<h1>Upcoming Games</h1>
<div class="games-list">
    <% upcomingGames.forEach(game => { %>
        <div class="game-item">
            <h2><a href="/game/<%= game.id %>"><%= game.title %></a></h2>
            <p>Release Year: <%= game.year %></p>
            <p><%= game.description %></p>
        </div>
    <% }) %>
</div>